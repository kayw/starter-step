{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
     __CLIENT__: false,
    "__DEV__": false,
    "__PROD__": false,
    "__SERVER__": false,
    "__API_ROOT__": false,
    "__DEVTOOLS__": false,
    "r": true,
    "webpackIsomorphicTools": true
  },
  "rules": {
    "comma-dangle": [1, "never"],
    "no-else-return": 0,
    "no-param-reassign": 0,
    "react/prefer-es6-class": 0,
    "new-cap": 0,
    "max-len": 0,
    "react/jsx-closing-bracket-location": 0,
    "object-curly-spacing": 0,
    "array-bracket-spacing": 0,
    "indent": 0,
    "react/jsx-indent-props": 0,
    "arrow-body-style": 0,
    "quote-props": 0,
    "react/jsx-no-bind": 0
  }
}
