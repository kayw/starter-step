{
  "name": "gu-ide",
  "version": "0.0.1",
  "description": "GuIDE web playground bookmarks",
  "main": "server/index.js",
  "scripts": {
    "clean": "rm -rf public/dist & rm -f public/assets.json",
    "lint": "eslint --fix --ext .js --ext .jsx client server universal webpack",
    "hot": "webpack-dev-server --config webpack/dev.js",
    "dev": "pm2 start pm2/dev.json --no-daemon",
    "start": "pm2 start pm2/dev.json --no-daemon & npm run hot",
    "build": "npm run clean && NODE_ENV=production webpack --progress --verbose --colors --display-error-details --profile --diplay-modules --display-origins --config webpack/prod.js",
    "server": "NODE_ENV=production node .",
    "prod": "npm run build && npm run server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "stage": 0
  },
  "author": "kayw",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.35",
    "babel-plugin-typecheck": "^1.3.0",
    "co-body": "^4.0.0",
    "debug": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "history": "^2.0.0",
    "immutable": "^3.7.6",
    "invariant": "^2.1.1",
    "koa": "^1.0.0",
    "koa-static": "^2.0.0",
    "material-ui": "^0.14.4",
    "pretty-error": "^2.0.0",
    "react": "^0.14.7",
    "react-css-modules": "^3.7.4",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.2",
    "react-dom": "^0.14.7",
    "react-redux": "^4.3.0",
    "react-router": "^2.0.0-rc5",
    "react-tap-event-plugin": "^0.2.2",
    "redbox-react": "^1.2.2",
    "redux": "^3.2.1",
    "redux-router": "^1.0.0-beta7",
    "rethinkdbdash": "^2.2.17",
    "serialize-javascript": "^1.1.2",
    "superagent": "^1.7.2",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^5.8.35",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^5.4.0",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.35",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.16.1",
    "postcss-loader": "^0.8.0",
    "postcss-scss": "^0.1.3",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redux-devtools": "^3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.4",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "webpack-isomorphic-tools": "^2.2.16",
    "yadc": "0.0.1"
  }
}
