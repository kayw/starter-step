{
  "name": "gu-ide",
  "version": "0.0.1",
  "description": "GuIDE web playground bookmarks",
  "main": "server/index.js",
  "scripts": {
    "clean": "rm -rf dist & rm -f webpack/assets.json",
    "build": "NODE_ENV=production webpack --progress --profile --diplay-modules --display-origins --config webpack/prod.js",
    "hot": "webpack-dev-server --config webpack/dev.js",
    "lint": "eslint --ext .js --ext .jsx client server universal webpack",
    "server": "node",
    "start": "npm run clean & pm2 start pm2/dev.json --no-daemon & npm run hot",
    "prod": "npm run clean && npm run build && npm run server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "stage": 0
  },
  "author": "kayw",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.23",
    "babel-plugin-typecheck": "^1.3.0",
    "co-body": "^4.0.0",
    "debug": "^2.2.0",
    "history": "^1.9.1",
    "immutable": "^3.7.5",
    "invariant": "^2.1.1",
    "koa": "^1.0.0",
    "koa-static": "^1.4.9",
    "material-ui": "^0.12.0",
    "pretty-error": "^1.2.0",
    "react": "^0.14.0",
    "react-css-modules": "^3.6.1",
    "react-document-meta": "^2.0.0-rc2",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc1",
    "react-tap-event-plugin": "^0.2.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux": "^3.0.2",
    "rethinkdbdash": "^2.1.8",
    "serialize-javascript": "^1.1.2",
    "superagent": "^1.4.0",
    "yargs": "^3.25.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
