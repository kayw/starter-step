{
  "name": "gu-ide",
  "version": "0.0.1",
  "description": "GuIDE web playground bookmarks",
  "main": "server/index.js",
  "scripts": {
    "clean": "rm -rf public/dist & rm -f public/assets.json",
    "lint": "eslint --fix --ext .js --ext .jsx client server universal webpack",
    "hot": "webpack-dev-server --config webpack/dev.js",
    "dev": "pm2 start pm2/dev.json --no-daemon",
    "start": "pm2 start pm2/dev.json --no-daemon & npm run hot",
    "build": "npm run clean && NODE_ENV=production webpack --progress --verbose --colors --display-error-details --profile --diplay-modules --display-origins --config webpack/prod.js",
    "server": "NODE_ENV=production node .",
    "prod": "npm run build && npm run server",
    "test": "NODE_ENV=test karma start tests/karma.conf"
  },
  "author": "kayw",
  "license": "MIT",
  "dependencies": {
