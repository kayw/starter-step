    "koa": "^1.2.0",
    "koa-static": "^2.0.0",
    "material-ui": "^0.15.1",
    "pretty-error": "^2.0.0",
    "react": "^15.1.0",
    "react-css-modules": "^3.7.6",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
